{
  "color": {
    "background": {
      "$type": "color",
      "$value": { "colorSpace": "srgb", "components": [1, 1, 1] }
    },
    "primary": {
      "$type": "color",
      "$value": { "colorSpace": "srgb", "components": [0, 0.4, 1] }
    }
  },
  "spacing": {
    "sm": { "$type": "dimension", "$value": { "value": 8, "unit": "px" } },
    "md": { "$type": "dimension", "$value": { "value": 16, "unit": "px" } }
  }
}
