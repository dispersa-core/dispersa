{
  "color": {
    "base": {
      "background": {
        "$type": "color",
        "$value": {
          "colorSpace": "srgb",
          "components": [1, 1, 1]
        }
      },
      "text": {
        "$type": "color",
        "$value": {
          "colorSpace": "srgb",
          "components": [0.067, 0.094, 0.153]
        }
      }
    }
  },
  "shadow": {
    "elevation": {
      "md": {
        "$type": "shadow",
        "$value": {
          "color": {
            "colorSpace": "srgb",
            "components": [0, 0, 0],
            "alpha": 0.1
          },
          "offsetX": {
            "value": 0,
            "unit": "px"
          },
          "offsetY": {
            "value": 4,
            "unit": "px"
          },
          "blur": {
            "value": 6,
            "unit": "px"
          },
          "spread": {
            "value": -1,
            "unit": "px"
          }
        }
      }
    }
  }
}
